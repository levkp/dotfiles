Host personal.gitlab.com
  HostName gitlab.com
  IdentityFile ~/.ssh/id_gitlab

Host work.gitlab.com
  HostName gitlab.com
  IdentityFile ~/.ssh/id_gitlab_work

Host 192.168.0.236
  User root
  Port 2222
  IdentityFile ~/.ssh/id_touchhd
  HostKeyAlgorithms +ssh-rsa,ssh-dss
  PubkeyAcceptedKeyTypes=+ssh-rsa,ssh-dss
  PasswordAuthentication no
  ForwardX11 no
  ControlMaster no
  ControlPersist no
